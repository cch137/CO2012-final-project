{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc build active file",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "db/api.c",
        "db/core.c",
        "db/hash.c",
        "db/interaction.c",
        "db/list.c",
        "db/obj.c",
        "db/utils.c",
        "db/zset.c",
        "db/deps/cJSON.c",
        "algorithms.c",
        "database.c",
        "social_network.c",
        "-lm"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
